public class Main {
	static String[] endSide = new String[6];
	static String[] startSide = new String[6];
	static String[] boat = new String[2];
	static String[] commands = new String[25];

	public static void main(String[] args) {
		resetToInitial();
		while (!isGoalMet()) {

		}
	}

	private static void resetToInitial() {
		startSide[0] = "C";
		startSide[1] = "C";
		startSide[2] = "C";
		startSide[3] = "M";
		startSide[4] = "M";
		startSide[5] = "M";
		endSide[0] = "";
		endSide[1] = "";
		endSide[2] = "";
		endSide[3] = "";
		endSide[4] = "";
		endSide[5] = "";
		boat[0] = "";
		boat[1] = "";
	}

	private static boolean isGoalMet() {
		if (!missionaryMajority(endSide)) {
			return false;
		} else {
			return missionaryMajority(startSide);
		}
	}

	private static boolean missionaryMajority(String[] side) {
		int c = 0, m = 0;
		for (String str : side) {
			if (str.equals("C"))
				c++;
			else if (str.equals("M")) {
				m++;
			}
		}
		if (c > m) {
			return false;
		}
		return true;
	}

	private static boolean isDone() {
		int c = 0, m = 0;
		for (String str : endSide) {
			if (str.equals("C"))
				c++;
			else if (str.equals("M")) {
				m++;
			}
		}
		if (c == 3 && m == 3) {
			return true;
		}
	}
}
